<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="400.0" minWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="paperMaster.PaperMasterController">
   <children>
      <ButtonBar layoutY="245.0" prefHeight="42.0" prefWidth="394.0">
        <buttons>
          <Button mnemonicParsing="false" onAction="#setNew" text="new" />
            <Button mnemonicParsing="false" onAction="#clearAll" text="clear" />
            <Button mnemonicParsing="false" onAction="#setUpdate" text="update" />
            <Button mnemonicParsing="false" onAction="#setRemove" text="remove" />
        </buttons>
      </ButtonBar>
      <Label layoutX="91.0" layoutY="86.0" text="paper title" />
      <Label layoutX="91.0" layoutY="148.0" text="price" />
      <ComboBox fx:id="comboTitle" editable="true" layoutX="91.0" layoutY="108.0" prefWidth="150.0" />
      <TextField fx:id="txtPrice" layoutX="91.0" layoutY="174.0" />
      <Label layoutX="78.0" layoutY="41.0" text="New Newpaper Entry" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <Button layoutX="285.0" layoutY="108.0" mnemonicParsing="false" onAction="#getTitle" text="fetch" />
      <Label layoutX="90.0" layoutY="317.0" text="Info:" />
      <Label fx:id="lblInfo" layoutX="125.0" layoutY="317.0" prefHeight="9.0" prefWidth="265.0" />
   </children>
</AnchorPane>
